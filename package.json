{
  "name": "api",
  "version": "1.0.0",
  "description": "campament L' Estel api to access into mongodb ",
  "main": "src/index.js",
  "scripts": {
    "cleanBabelCache": "rm -rf ./node_modules/.cache/babel-loader",
    "clean": "npm run cleanBabelCache && rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build-babel",
    "build-babel": "babel src -d dist",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --watch src --exec npm start",
    "seeds": "node ./dist/seeds",
    "test": "jest"
  },
  "author": "josue",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/register": "^7.10.5",
    "@babel/runtime": "^7.11.2",
    "babel-plugin-import-graphql": "^2.7.0",
    "faker": "^4.1.0",
    "jest": "^26.4.0",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "@babel/preset-env": "^7.11.0",
    "apollo-server-express": "^2.16.1",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.1",
    "graphql": "^14.7.0",
    "graphql-passport": "^0.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "merge-graphql-schemas": "^1.7.8",
